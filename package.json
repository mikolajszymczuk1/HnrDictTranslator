{
  "name": "hnrdicttranslator",
  "displayName": "HnrDictTranslator",
  "description": "Translate any key:value dict objects to other languages in few seconds ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "authors": [
    "Mikołaj Szymczuk",
    "Kacper Kukuła"
  ],
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "HnrDictTranslator",
      "properties": {
          "hnrdicttranslator.deeplApiKey": {
              "type": "string",
              "default": ""
          }
      }
    },
    "commands": [
      {
        "command": "hnrdicttranslator.hnrtranslate",
        "title": "Hnr Translate"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.8",
    "eslint": "^8.56.0",
    "husky": "^9.0.6",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "deepl-node": "^1.11.0"
  }
}
